{
  "file":{
    "name":"file",
    "help":"File handeler Tool",
    "args":{
        "-d": {
          "help":"Duplicate [PATH]"
        },
        "-b": {
          "help":"Backup [PATH]"
        }
    }
  },
  "job_applied":{
    "help":"Save a job you just applied for",
    "args":{
        "-c": {
          "help":"Company Name",
          "action": "store",
          "required": "True"
        },
        "-p": {
          "help":"Position",
          "action": "store",
          "required": "True"
        },
        "--url": {
          "help":"URL",
          "action": "store",
          "required": "True"
        }
    }
  },
  "-V":{
    "help":"Verbose",
    "action": "store_true"
  }
}
